/* 主页跳转动画 */
.app-transform-leave-active,
.app-transform-enter-active {
  transition: all 0.28s;
}
.app-transform-enter {
  opacity: 0;
  transform: translateY(-30px) !important;
}
.app-transform-leave-to {
  opacity: 0;
  transform: translateY(30px) !important;
}
/* fade */
.fade-enter-active,
.fade-leave-active {
  transition: opacity 0.28s ease;
}
.fade-enter,
.fade-leave-active {
  opacity: 0;
}
/* fade-transform */
.fade-transform-leave-active,
.fade-transform-enter-active {
  transition: all 0.28s;
}
.fade-transform-enter {
  opacity: 0;
  transform: translateX(-30px) !important;
}
.fade-transform-leave-to {
  opacity: 0;
  transform: translateX(30px) !important;
}
/* breadcrumb transition */
.breadcrumb-enter-active,
.breadcrumb-leave-active {
  transition: all 0.5s;
}
.breadcrumb-enter,
.breadcrumb-leave-active {
  opacity: 0;
  transform: translateX(20px);
}
.breadcrumb-move {
  transition: all 0.5s;
}
.breadcrumb-leave-active {
  position: absolute;
}
