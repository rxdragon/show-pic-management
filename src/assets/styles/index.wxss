.orange-button {
  color: #fff;
  background: linear-gradient(90deg, #ffab3f 0%, #ff7a00 100%);
  border-color: #ffab3f;
}
.orange-button:focus,
.orange-button:hover {
  color: #fff;
  background: linear-gradient(90deg, #ffab3f 0%, #ff7a00 100%);
  border-color: #ffab3f;
}
.plant-button-text {
  color: #44C27E;
  border-color: #44C27E;
}
.plant-button-text:focus,
.plant-button-text:hover {
  color: #fff !important;
  background-color: #44C27E !important;
  border-color: #44C27E !important;
}
.num-input {
  width: 100%;
  font-size: 14px;
  text-align: center;
  border: none;
  outline: none;
}
.fake-el-input {
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  display: inline-block;
  width: 100%;
  height: 28px;
  padding: 0 15px;
  font-size: inherit;
  line-height: 28px;
  color: #606266;
  background-color: #fff;
  background-image: none;
  border: 1px solid #dcdfe6;
  border-radius: 4px;
  outline: 0;
  -webkit-transition: border-color 0.2s cubic-bezier(0.645, 0.045, 0.355, 1);
  transition: border-color 0.2s cubic-bezier(0.645, 0.045, 0.355, 1);
  -webkit-appearance: none;
}
.fake-el-input:focus {
  border-color: #4669FB;
  outline: 0;
}
.download-popover {
  padding: 20px;
  border-radius: 20px;
}
.content-main {
  height: calc(100vh - 64px);
  overflow-y: overlay;
}
.standard-icon {
  display: inline-block;
  width: 16px;
  height: 17px;
  margin-left: 5px;
  vertical-align: text-top;
}
.standard-box {
  display: flex;
  align-items: center;
}
